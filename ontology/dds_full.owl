    <!-- http://inf.mit.bme.hu/research/dds/Subscriber -->

    <owl:Class rdf:about="http://inf.mit.bme.hu/research/dds/Subscriber">
        <rdfs:subClassOf rdf:resource="http://inf.mit.bme.hu/research/dds/DomainParticipant"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://inf.mit.bme.hu/research/dds/has_DataReader"/>
                <owl:allValuesFrom rdf:resource="http://inf.mit.bme.hu/research/dds/DataReader"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
